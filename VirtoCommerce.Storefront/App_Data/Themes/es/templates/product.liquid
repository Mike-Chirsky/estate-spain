<div class="row">
  <div class="header_media">
    <div class="product-preview">
        {% for image in product.images %}
        {% if forloop.index > 3 %}
        {% break %}
        {% endif %}
        <div class="preview-item">
          <div class="preview-inner">
            <div class="preview-inner-image" style="background-image: url('{{ image.src | img_url: 'thumb' }}');"></div>
          </div>
        </div>
        {% endfor %}
    </div>
  </div>
    <div class="row">
    {% include 'breadcrumb' %}
    </div>
    <div class="col-md-9 rightmargin full_width_prop">
        <h1 class="entry-title entry-prop">
           {{ product.custom_title }}
        </h1>
        <span class="price_area"><span class="price_label price_label_before"></span>{% include 'price' with product.price %} </span>
        <div class="single-content listing-content">
            <div class="notice_area">
                <div class="property_categs">
                  <div>

                    {% for property in product.properties %}
                    {% if property.name == 'region' %}
                    {{ property.value}}
                    <span class="divider" >›</span>
                    {%endif%}
                    {% endfor %}

                    {% for property in product.properties %}
                    {% if property.name == 'district' %}
                    {{ property.value}}
                    <span class="divider" >›</span>
                    {%endif%}
                    {% endfor %}
                    
                    {% for property in product.properties %}
                    {% if property.name == 'city' %}
                    {{ property.value}}
                    {%endif%}
                    {% endfor %}
                  </div>
                  <div>
                    {% for property in product.properties %}
                    {% if property.name == 'h2' %}
                    {{ property.value}}
                    {%endif%}
                    {% endfor %}
                    <span class="deal-type">
                    {% for property in product.properties %}
                    {% if property.name == 'dealtype' %}
                    {{ property.value}}
                    {%endif%}
                    {% endfor %}
                    </span>
                  </div>
                </div>
                <!--<div id="add_favorites" class="isnotfavorite" data-postid="139">add to favorites</div>
                <div class="download_pdf"></div>-->
                <div class="prop_social">
                    <a href="http://www.odnoklassniki.ru/dk?st.cmd=addShare&st.s=1&st._surl={{ product.url | full_url }}" target="blank" class="share_ok"><i class="fa fa-odnoklassniki fa-2"></i></a>
                    <a href="https://vk.com/share.php?url={{ product.url | full_url }}" target="blank" class="share_vk"><i class="fa fa-vk fa-2"></i></a>
                    <a href="http://www.facebook.com/sharer.php?u={{ product.url | full_url }}" target="_blank" class="share_facebook"><i class="fa fa-facebook fa-2"></i></a>
                    <a href="http://twitter.com/home?status={{ product.url | full_url }}" class="share_tweet" target="_blank"><i class="fa fa-twitter fa-2"></i></a>
                    <a href="https://plus.google.com/share?url={{ product.url | full_url }}" target="_blank" class="share_google"><i class="fa fa-google-plus fa-2"></i></a>
                </div>
            </div>
            <div class="wpestate_property_description">
                <p>{{ product.title }}</p>
                <p>{{ product.description }}</p>
            </div>
            <div class="panel-group property-panel" id="accordion_prop_details">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <a data-toggle="collapse" data-parent="#accordion_prop_details" href="#collapseProperties">
                            <h4 class="panel-title" id="prop_det">{{ 'product.description' | t }}</h4>
                        </a>
                    </div>
                    <div id="collapseProperties" class="panel-collapse collapse in">
                        <div class="panel-body">
                            {% include 'properties/detail' %}
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-group property-panel" id="accordion_prop_full_details">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <a data-toggle="collapse" data-parent="#accordion_prop_full_details" href="#collapseFullDeatils">
                            <h4 class="panel-title" id="prop_det">{{ 'product.full-info' | t }}</h4>
                        </a>
                    </div>
                    <div id="collapseFullDeatils" class="panel-collapse collapse in">
                        <div class="panel-body">
                            {% include 'properties/info' %}
                        </div>
                    </div>
                </div>
            </div>
            <div id="carousel-listing" class="carousel slide post-carusel" data-ride="carousel" data-interval="false">
                <div>
                <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        {% for image in product.images %}
                        {% if forloop.first %}
                        <div class="item active">
                        {% else %}
                        <div class="item">
                        {% endif %}
                            <a href="{{ image.src }}" title="" rel="prettyPhoto" class="prettygalery">
                                <img src="{{ image.src | img_url: 'large' }}" data-slider-no="{{ forloop.index | minus: 1 }}" alt="" class="img-responsive lightbox_trigger">
                            </a>
                        </div>
                        {% endfor %}
                    </div>

                    <div class="slider-information">
                        <div><span id="current-main-slide">1</span> / {{ product.images.size }}</div>
                    </div>    
                    <!-- Controls -->
                    <a class="left carousel-control" href="#carousel-listing" data-slide="prev">
                        <i class="fa fa-angle-left"></i>
                    </a>
                    <a class="right carousel-control" href="#carousel-listing" data-slide="next">
                        <i class="fa fa-angle-right"></i>
                    </a>
                </div>
                <!-- Indicators -->
                <div class="carusel-back"></div>
                <ol class="carousel-indicators">
                    {% for image in product.images %}
                    {% if forloop.first %}
                    <li data-target="#carousel-listing" data-slide-to="{{ forloop.index | minus: 1 }}" class="active">
                    {% else %}
                    <li data-target="#carousel-listing" data-slide-to="{{ forloop.index | minus: 1 }}" class="">
                    {% endif %}
                        <img src="{{ image.src | img_url: 'small' }}" alt="slider">
                    </li>
                    {% endfor %}
                </ol>
            </div>
            {% for property in product.properties %}
                {% if property.name == 'geopoints' %}
                    {% assign values = property.value | split: ','  %}
                    {% for degree in values %}
                        {% if forloop.first %}
                            {% assign lat = degree %}
                        {% else %}
                            {% assign lon = degree %}
                        {% endif %}
                    {% endfor %}
                    {% break %}
                {% endif %}
            {% endfor %}
            <div class="panel-group property-panel" id="accordion_map">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <a data-toggle="collapse" data-parent="#accordion_map" href="#collapseMap">
                            <h4 class="panel-title" id="prop_det">{{ 'product.map' | t }}</h4>
                        </a>
                    </div>
                    <div id="collapseMap" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <!-- Google Map -->
                            <div id="gmap_wrapper" data-post_id="" data-cur_lat="{{ lon }}" data-cur_long="{{ lat }}" style="height:400px">
                                <div id="googleMap" style="height:400px">
                                </div>
                                <div class="tooltip"> {{ 'map.enable-zoom' | t }}</div>
                                <div id="gmap-loading">
                                    {{ 'map.loading' | t }}
                                    <div class="spinner map_loader" id="listing_loader_maps">
                                        <div class="rect1"></div>
                                        <div class="rect2"></div>
                                        <div class="rect3"></div>
                                        <div class="rect4"></div>
                                        <div class="rect5"></div>
                                    </div>
                                </div>
                                <div id="gmap-noresult">
                                    {{ 'map.no-result' | t }}
                                </div>
                                <div class="gmap-controls">
                                  <!--  <div id="gmap-control">
                                        <span id="map-view"><i class="fa fa-picture-o"></i>{{ 'map.view' | t }}</span>
                                        <span id="map-view-roadmap" class="map-type">{{ 'map.roadmap' | t }}</span>
                                        <span id="map-view-satellite" class="map-type">{{ 'map.satellite' | t }}</span>
                                        <span id="map-view-hybrid" class="map-type">{{ 'map.hybrid' | t }}</span>
                                        <span id="map-view-terrain" class="map-type">{{ 'map.terrain' | t }}</span>
                                        <span id="geolocation-button"><i class="fa fa-map-marker"></i>{{ 'map.my-location' | t }}</span>
                                        <span id="gmap-full"><i class="fa fa-arrows-alt"></i>{{ 'map.fullscreen' | t }}</span>
                                        <span id="gmap-prev"><i class="fa fa-chevron-left"></i>{{ 'map.prev' | t }}</span>
                                        <span id="gmap-next">{{ 'map.next' | t }}<i class="fa fa-chevron-right"></i></span>
                                    </div>-->
                                    <div id="gmapzoomplus"><i class="fa fa-plus"></i> </div>
                                    <div id="gmapzoomminus"><i class="fa fa-minus"></i></div>
                                    <!--<div id="street-view"><i class="fa fa-location-arrow"></i> {{ 'map.streetview' | t }} </div>-->
                                </div>
                            </div>
                            <!-- END Google Map -->
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-group property-panel" id="accordion_iteractive">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <a data-toggle="collapse" data-parent="#accordion_iteractive" href="#collapseIteractive">
                            <h4 class="panel-title" id="prop_det">{{ 'product.interactive' | t }}</h4>
                        </a>
                    </div>
                    <div id="collapseIteractive" class="panel-collapse collapse in">
                        <div class="panel-body">
                            Интерактивные видео
                        </div>
                    </div>
                </div>
            </div>
            
            <div class="agent_contanct_form addition-form">
                <h4 id="show_contact">{{ 'request-form.header2' | t }}</h4>
                <div class="row">
                    <div class="col-md-4">
                        <input name="contact_name" type="text" placeholder="{{ 'request-form.name' | t }}" aria-required="true" class="form-control">
                    </div>
                    <div class="col-md-4">
                        <input type="text" name="phone" class="form-control"  placeholder="{{ 'request-form.phone' | t }}">
