                    <div class="col-md-4">
                        <input type="text" name="email" class="form-control" aria-required="true" placeholder="{{ 'request-form.mail' | t }}">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <textarea id="agent_comment" name="comment" class="form-control" cols="45" rows="8" aria-required="true">{{ 'request-form.message' | t }}</textarea>    
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                      <span class="license"> {{ 'request-form.license' | t }} </span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        
                        <input type="submit" class="wpresidence_button agent_submit_class" id="agent_submit" value="{{ 'request-form.send' | t }}">
                    </div>
                </div>                
            </div>
        </div>
        <!-- end single content -->
    </div>
    <!--Side bar-->
