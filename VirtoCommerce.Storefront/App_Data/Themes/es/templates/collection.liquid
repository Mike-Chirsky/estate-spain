<div class="breadcrumb_container">
  {% include 'breadcrumb' %} 
</div>
{%- if from_filter == true and (collection.type == "region" or collection.type == "type" or collection.type == "type_add") -%}
  <div class="row">
    <h2 class="text-center">{{-'your-request2' | t -}}&nbsp;{%- include 'product-count' -%}&nbsp;{{-'objects' | t -}}</h2>
  </div>
  {% include 'product-list' %}
{%endif%}

{% if collection.handle != 'arenda' %}
  {% include 'headers/header-about' %}
{% endif %}
{%- if collection.type == 'region' or collection.type == 'type' or collection.type == 'type_add' -%}
  {% include 'headers/region-header' %}
{%- endif -%}

{%- if collection.type == 'city' -%}
  {% include 'headers/city-header' %}
{%- endif -%}

{%- if collection.type == 'region' or collection.type == 'type' or collection.type == 'type_add' -%}
  market
  {% include 'headers/header-name' %}
  {% case collection.type %}
    {% when 'type_add' %}
      {% include 'market-block/market-block', market_id: collection.title, market_url: 'storefrontapi/market/type', block_id: collection.handle %}
    {% when 'type' %}
      {% include 'market-block/market-block', market_id: collection.title, market_url: 'storefrontapi/market/type', block_id: collection.handle %}
    {% when 'region'%}
      {% include 'market-block/market-block', market_id: collection.title, market_url: 'storefrontapi/market/region', block_id: collection.handle %}
    {% when 'city'%}
      {% include 'market-block/market-block', market_id: collection.title, market_url: 'storefrontapi/market/city', block_id: collection.handle %}
  {% endcase%}
{%- endif -%}

{%- if collection.type == 'city' or collection.handle == 'arenda' or collection.type == 'type_add' -%}
  <h2 class="header-name text-center">{{- collection.h2_listing -}}</h2>
  <h4 class="text-center">{{'found' | t }} {% include 'product-count' %} {{ 'objects' | t }}</h4>
  {% include 'product-list' %}
{%- endif -%}

{%- if collection.type == 'region' or collection.type == 'type'-%}
  footer
  {% include 'footer/region' %}
{%- endif -%}
{% if collection.handle != 'arenda' %}
  {% include 'footer/about' %}
{% endif %}
{% include 'blog/blog-collection-list' %}
<div class="row contact-us-footer">
{% include 'footer/contact-us' %}
</div>
<div class="callback-form">
{% include 'footer/request-call' %}
</div>